<view class="my">
  <view class="login">
    <view class="profile-img">
      <open-data type="userAvatarUrl"></open-data>
    </view>
    <open-data type="userNickName" class="profile-text"></open-data>
  </view>
  <view class="content">
    <view class="content_item" bindtap="bathingOrder">
      <view bindtap="bathingOrder">预约洗护</view>
      <image src="../../image/more.png"></image>
    </view>
    <view class="content_item" bindtap="bathingrecords">
      <view bindtap="bathingrecords">洗护记录</view>
      <image src="../../image/more.png"></image>
    </view>
    <view class="content_item" bindtap="bathcomment">
      <view>意见反馈</view>
      <image src="../../image/more.png"></image>
    </view>
  </view>
  <view class="profile"></view>
</view>